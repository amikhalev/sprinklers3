    "moment": "^2.22.0",
    "mqtt": "^2.17.0",
    "pino": "^4.16.1",
    "postcss-cssnext": "^3.1.0",
    "serializr": "^1.1.14",
    "uglify-es": "3.3.9",
    "ws": "^5.1.1"
  },
