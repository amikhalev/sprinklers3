    "@types/react-hot-loader": "^3.0.4",
    "autoprefixer": "^7.1.4",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "classnames": "^2.2.5",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-pino-logger": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "font-awesome": "^4.7.0",
