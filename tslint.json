{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "no-console": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "ordered-imports": false,
    "variable-name": [
      "allow-leading-underscore"
    ],
    "no-namespace": [
      "allow-declarations"
    ],
    "interface-name": false,
    "member-access": [ true, "no-public" ]
  },
  "rulesDirectory": []
}