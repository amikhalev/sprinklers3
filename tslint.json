{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "no-console": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "ordered-imports": true,
    "variable-name": [
      "allow-leading-underscore"
    ],
    "no-namespace": false,
    "interface-name": false,
    "member-access": [
      true,
      "no-public"
    ],
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ]
  },
  "rulesDirectory": []
}
