{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "variable-name": [
            "allow-leading-underscore"
        ],
        "no-namespace": [
            "allow-declarations"
        ]
    },
    "rulesDirectory": []
}